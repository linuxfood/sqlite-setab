[cxx]
  cc     = /usr/bin/gcc-5
  cpp    = /usr/bin/gcc-5
  aspp   = /usr/bin/gcc-5

  cxx    = /usr/bin/g++-5
  cxxpp  = /usr/bin/g++-5
  ld     = /usr/bin/g++-5
  cxxflags = -std=c++14 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -fno-omit-frame-pointer -O3 -Wall -g -pthread
  ldflags = -pthread -Wl,-rpath=/usr/local/lib/
